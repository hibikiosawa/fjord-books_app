<h2>日報一覧</h2>
<hr>
<% @reports.each do |report| %>
    <li><%= Report.human_attribute_name(:user) %>
    <%= report.user_id %></li>
    <li><%= Report.human_attribute_name(:title) %>
    <%= report.title %></li>
    <li><%= Report.human_attribute_name(:body) %>
    <%= report.body %></li>
    <%= link_to t('views.common.report_show'), edit_report_path(report) %>
    <hr>
<% end %>
<%= paginate @reports %>